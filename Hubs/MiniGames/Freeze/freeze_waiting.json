{
  "Key": "freeze_waiting",
  "Author": "SharkeyMyrl",
  "Description": "Lobby for freeze game.",

  "SendMessage": "Entering Lobby",

  "Start": "lobby",
  "Start": "lobby_spectator",

  "Start": "freeze",
  "Start": "freeze_scoreboard",

  "Comment": "Loop",

  "If": [],
  "Equals": ["server", "freeze_game_on", ">=", 1.0],
  "Skip": "SendToTeams",
  "Skip": "PickTeams",

  "Comment": "SendToTeams",

  "If": [],
  "In": ["freeze_spectator", false],
  "Skip": "ProgSpec",
  "Set": ["team", "rand"],
  "Skip": "Random",

  "Comment": "ProgSpec",
  "Start": "freeze_schematic",
  "Progress": "freeze_spectator",
  "Leave": [],

  "Comment": "PickTeams",
  "Wait": 1,
  "If": [],
  "Equals": ["team", "spectator"],
  "Skip": "Spectator",
  "Leave": "Spectator",
  "Set": ["team", ""],
  "Skip": "Loop",

  "Comment": "Random",
  "If": [],
  "Equals": ["[SCRIPT_PLAYER_COUNT]", "freeze_blue", "<=", 0.0],
  "Start": "freeze_blue",
  "Start": "freeze_red",
  "Hold": [],

  "Comment": "Spectator",
  "If": [],
  "In": ["freeze_spectator", false],
  "Skip": "Loop",
  "Dummy": [],

  "Leave": "freeze_player",
  "Leave": "freeze_red",
  "Leave": "freeze_blue",
  "Start": "freeze_spectator",
  "Set": ["team", ""],
  "Skip": "Loop",

  "Dummy": []
}
