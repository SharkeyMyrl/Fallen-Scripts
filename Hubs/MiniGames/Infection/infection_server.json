{
  "Key": "infection_server",

  "Repeat": 10,

  "If": [],
  "Equals": ["server", "infection_timer", "=", -1.0],
  "Dummy": [],
  "Skip": "GameTimer",

  "Comment": "LobbyTimer",
  "If": [],
  "Equals": ["[SCRIPT_PLAYER_COUNT]", "infection_waiting", "=", 0.0],
  "Skip": "NoLobby",
  "Skip": "LobbyOn",

  "Comment": "NoLobby",
  "If": [],
  "Equals":["server", "infection_lobby_timer", "=", -1.0],
  "Wait": 220,
  "Set": ["server", "infection_lobby_timer", -1.0],
  "Wait": 220,
  "Comment":"LobbyOn",
  "Add": ["server", "infection_lobby_timer", 1.0],

  "If": [],
  "Equals": ["server", "infection_lobby_timer", ">=", 9.0],
  "Dummy": [],
  "Wait": 220,

  "Comment": "GameTimer",
  "Comment": "Is anyone playing or has game even started",
  "If":[],
  "Equals": ["[SCRIPT_PLAYER_COUNT]", "infection", "=", 0.0],
  "Skip": "NoGame",
  "Skip": "GameOn",

  "Comment": "NoGame",
  "Set": ["server", "infection_timer", -1.0],
  "Set": ["server", "infection_timer_max", 18.0],
  "Wait": 220,
  "Comment": "GameOn",
  "Add": ["server", "infection_timer", 1.0],

  "Comment": "Did timer go over?",
  "If": [],
  "Equals": ["server", "infection_timer", ">=", "[server:infection_timer_max]"],
  "Skip": "BlueWins",
  "Dummy": [],

  "Comment": "Did red lose?",
  "If":[],
  "Equals": ["[SCRIPT_PLAYER_COUNT]", "infection_red", "=", 0.0],
  "Skip": "BlueWins",
  "Dummy": [],

  "Comment": "Did blue lose?",
  "If":[],
  "Equals": ["[SCRIPT_PLAYER_COUNT]", "infection_blue", "=", 0.0],
  "Skip": "RedWins",
  "Dummy": [],

  "Comment": "BlueWins",
  "StartAll": [1.0, 7, "infection_winners"],
  "Skip": "CleanUp",

  "Comment": "RedWins",
  "StartAll": [1.0, 2, "infection_winners"],
  "Skip": "Cleanup",

  "Comment": "CleanUp",
  "LeaveAll": ["infection_blue", "infection_blue_slave"],
  "LeaveAll": ["infection_red", "infection_red_slave"],
  "Set": ["server", "infection_timer", -1.0],



  "Dummy": []
}
