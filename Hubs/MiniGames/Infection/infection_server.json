{
  "Key": "infection_server",
  "Author": "SharkeyMyrl",

  "Repeat": 10.0,

  "If": [],
  "Equals": ["server", "infection_game_on", "=", 1.0],
  "Skip": "GameTimer",
  "Wait": 1.0,

  "Comment": "LobbyTimer",
  "If": [],
  "Equals": ["[SCRIPT_PLAYER_COUNT]", "infection_waiting", "<=", 2.0],
  "Skip": "NoLobby",
  "Skip": "LobbyOn",

  "Comment": "NoLobby",
  "If": [],
  "Equals":["server", "infection_lobby_timer", "=", -1.0],
  "Leave": [],
  "Set": ["server", "infection_lobby_timer", -1.0],
  "Leave": [],
  "Comment":"LobbyOn",
  "Add": ["server", "infection_lobby_timer", 1.0],

  "If": [],
  "Equals": ["server", "infection_lobby_timer", ">=", 9.0],
  "Wait": 1.0,
  "Leave": [],

  "Set": ["server", "infection_game_on", 1.0],

  "Comment": "GameTimer",
  "Comment": "Is anyone playing or has game even started",
  "If":[],
  "Equals": ["[SCRIPT_PLAYER_COUNT]", "infection_game_on", "=", -1.0],
  "Skip": "NoGame",
  "Skip": "GameOn",

  "Comment": "NoGame",
  "Set": ["server", "infection_timer", -1.0],
  "Set": ["server", "infection_timer_max", 18.0],
  "Leave": [],
  "Comment": "GameOn",
  "Add": ["server", "infection_timer", 1.0],

  "If": [],
  "Equals": ["server", "infection_timer", ">=", 6.0],
  "Wait": 1.0,
  "Leave": [],

  "Comment": "Did timer go over?",
  "If": [],
  "Equals": ["server", "infection_timer", ">=", 18.0],
  "Skip": "BlueWins",
  "Wait": 1.0,

  "Comment": "Did red lose?",
  "If":[],
  "Equals": ["[SCRIPT_PLAYER_COUNT]", "infection_red", "=", 0.0],
  "Skip": "BlueWins",
  "Wait": 1.0,

  "Comment": "Did blue lose?",
  "If":[],
  "Equals": ["[SCRIPT_PLAYER_COUNT]", "infection_blue", "=", 0.0],
  "Skip": "RedWins",
  "Wait": 1.0,

  "Comment": "BlueWins",
  "StartAll": [1.0, 8, "infection_winners"],
  "Skip": "CleanUp",

  "Comment": "RedWins",
  "StartAll": [1.0, 3, "infection_winners"],
  "Skip": "Cleanup",

  "Comment": "CleanUp",
  "LeaveAll": ["infection_blue_slave", "infection_red_slave", "infection_region", "infection_waiting", "infection_blue", "infection_red"],
  "Set": ["server", "infection_timer", -1.0],
  "Set": ["server", "infection_lobby_timer", -1.0],
  "Set": ["server", "infection_game_on", -1.0],



  "Dummy": []
}
