{
  "Key": "infection_server",
  "Author": "SharkeyMyrl",

  "Repeat": 10.0,

  "If": [],
  "Equals": ["server", "infection_game_on", "=", 1.0],
  "Skip": "GameTimer",
  "Dummy": [],

  "If": [],
  "Equals": ["[SCRIPT_PLAYER_COUNT]", "infection_waiting", "<=", 2.0],
  "Skip": "NoLobby",
  "Skip": "LobbyOn",

  "Comment": "NoLobby",
  "Set": ["server", "infection_lobby_timer", -1.0],
  "Leave": [],

  "Comment":"LobbyOn",
  "Add": ["server", "infection_lobby_timer", 1.0],

  "If": [],
  "Equals": ["server", "infection_lobby_timer", "<=", 9.0],
  "Leave": [],
  "Set": ["server", "infection_game_on", 1.0],

  "Comment": "GameTimer",
  "Comment": "Is anyone playing or has game even started",
  "If":[],
  "Equals": ["[SCRIPT_PLAYER_COUNT]", "infection_game_on", "=", -1.0],
  "Skip": "NoGame",
  "Skip": "GameOn",

  "Comment": "NoGame",
  "Set": ["server", "infection_timer", -1.0],
  "Set": ["server", "infection_timer_max", 18.0],
  "Leave": [],

  "Comment": "GameOn",
  "Add": ["server", "infection_timer", 1.0],

  "If": [],
  "Equals": ["server", "infection_timer", "<=", 2.0],
  "Leave": [],
  "Dummy": [],


  "If": [],
  "Equals": ["server", "infection_timer", ">=", 18.0],
  "Skip": "BlueWins",
  "Dummy": [],

  "If":[],
  "Equals": ["[SCRIPT_PLAYER_COUNT]", "infection_red", "=", 0.0],
  "Skip": "BlueWins",
  "Dummy": [],

  "If":[],
  "Equals": ["[SCRIPT_PLAYER_COUNT]", "infection_blue", "=", 0.0],
  "Skip": "RedWins",
  "Leave": [],

  "Comment": "BlueWins",
  "StartAll": [1.0, 7, "infection_winners"],
  "Skip": "CleanUp",

  "Comment": "RedWins",
  "StartAll": [1.0, 3, "infection_winners"],
  "Skip": "CleanUp",

  "Comment": "CleanUp",
  "Set": ["server", "infection_timer", -1.0],
  "Set": ["server", "infection_lobby_timer", -1.0],
  "Set": ["server", "infection_game_on", -1.0],



  "Dummy": []
}
