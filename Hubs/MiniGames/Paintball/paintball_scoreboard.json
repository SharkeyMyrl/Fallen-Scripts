{
  "Key": "paintball_scoreboard",

  "Comment": "Loop",

  "If": [],
  "In": ["paintball_waiting", false],
  "Skip": "Lobby",
  "Dummy": [],

  "If": [],
  "In": ["paintball_blue", false],
  "Skip": "TeamBlue",
  "Dummy": [],

  "If": [],
  "In": ["paintball_red", false],
  "Skip": "TeamRed",
  "Dummy": [],

  "Comment": "Lobby",
  "SetScoreboard": [
    "---PAINTBALL---",
    "---------------",
    "",
    "Lobby Timer: [server:paintball_lobby_timer]",
    "",
    "&cRed Team: &f[SCRIPT_PLAYER_COUNT:paintball_red]",
    "&9Blue Team: &f[SCRIPT_PLAYER_COUNT:paintball_blue]"
  ],
  "Wait": 10,
  "Skip": "Loop",

  "Comment": "TeamRed",
  "SetScoreboard": [
    "---PAINTBALL---",
    "---------------",
    "",
    "Game Timer: [server:paintball_timer]",
    "",
    "Round: &f[server:paintball_round]",
    "&cScore: &f[server:paintball_redwin]",
    "&9Score: &f[server:paintball_bluewin]",
    "",
    "&lPlayers",
    "&cTeam: &f[SCRIPT_PLAYER_COUNT:paintball_red]",
    "&9Team: &f[SCRIPT_PLAYER_COUNT:paintball_blue]"
  ],
  "Wait": 10,
  "Skip": "Loop",


  "Comment": "TeamBlue",
  "SetScoreboard": [
    "---PAINTBALL---",
    "---------------",
    "",
    "Game Timer: [server:paintball_timer]",
    "",
    "Round: &f[server:paintball_round]",
    "&9Score: &f[server:paintball_bluewin]",
    "&cScore: &f[server:paintball_redwin]",
    "",
    "&lPlayers",
    "&9Team: &f[SCRIPT_PLAYER_COUNT:paintball_blue]",
    "&cTeam: &f[SCRIPT_PLAYER_COUNT:paintball_red]"
  ],
  "Wait": 10,
  "Skip": "Loop",






  "Dummy": []
}
