{
  "Key": "paintball_waiting",
  "Author": "SharkeyMyrl",
  "Description": "Lobby for paintball game.",

  "SendMessage": "Entering Lobby",

  "Start": "lobby",

  "Start": "paintball",

  "Comment": "Loop",

  "If": [],
  "Equals": ["server", "paintball_game_on", ">=", 1.0],
  "Skip": "SendToTeams",
  "Skip": "PickTeams",

  "Comment": "SendToTeams",
  "Wait": 1,
  "If": [],
  "In": ["paintball_red", false],
  "Progress": "paintball_red",
  "Dummy": [],

  "If": [],
  "In": ["paintball_blue", false],
  "Progress": "paintball_blue",
  "Set": ["team", "rand"],
  "Skip": "PickTeams",

  "Leave": [],

  "Comment": "PickTeams",
  "Wait": 1,
  "If": [],
  "Equals": ["team", "red"],
  "Skip": "Red",
  "Dummy": [],

  "If": [],
  "Equals": ["team", "blue"],
  "Skip": "Blue",
  "Dummy": [],

  "If": [],
  "Equals": ["team", "rand"],
  "Skip": "Neutral",
  "Dummy": [],

  "If": [],
  "Equals": ["team", ""],
  "Skip": "Loop",
  "Dummy": [],

  "SendMessage": "How did you get here?",
  "Set": ["team", ""],
  "Skip": "Loop",

  "Comment": "Blue",
  "If": [],
  "Equals": ["[SCRIPT_PLAYER_COUNT]", "infection_red", ">=", "[SCRIPT_PLAYER_COUNT:infection_blue]"],
  "Skip": 2,
  "SendMessage": "Sorry but at this time the team you picked is full please pick again.",
  "Set": ["team", ""],
  "Skip": "Loop",

  "If": [],
  "In": ["paintball_blue", false],
  "Skip": "Loop",
  "Leave": "paintball_red",
  "Start": "paintball_blue",
  "Set": ["team", ""],
  "Skip": "Loop",

  "Comment": "Red",
  "If": [],
  "Equals": ["[SCRIPT_PLAYER_COUNT]", "infection_blue", ">=", "[SCRIPT_PLAYER_COUNT:infection_red]"],
  "Skip": 2,
  "SendMessage": "Sorry but at this time the team you picked is full please pick again.",
  "Set": ["team", ""],
  "Skip": "Loop",

  "If": [],
  "In": ["paintball_red", false],
  "Skip": "Loop",
  "Leave": "paintball_blue",
  "Start": "paintball_red",
  "Set": ["team", ""],
  "Skip": "Loop",

  "Comment": "Neutral",
  "Leave": "paintball_red",
  "Leave": "paintball_blue",

  "RandomOption": 2,
  "StartEmptiest": ["paintball_blue", "paintball_red"],
  "StartEmptiest": ["paintball_red", "paintball_blue"],

  "Set": ["team", ""],
  "Skip": "Loop",

  "Dummy": []
}
