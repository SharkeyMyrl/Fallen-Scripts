  "Comment": "GameOver_BlueWinner",
  "StartAll": [1.0, "Blue", "infection_winners"],
  "Skip": "CleanUp",

  "Comment": "CleanUp",
  "SetPersistent": ["infection", "red", 0],
  "SetPersistent": ["infection", "blue", 0],


  "Dummy": []
}
